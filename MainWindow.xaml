<Window x:Class="AOA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AOA"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="500" MaxHeight="500" MinWidth="900" MaxWidth="1000" Height="500" Width="900">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <Frame Grid.Column="0" Name="PageNavigation" NavigationUIVisibility="Hidden"/>
        <Frame Grid.Column="1" Name="AuthNavigation" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
